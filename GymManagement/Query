CREATE TABLE GYM_TB (
    RECORDID VARCHAR2(12 BYTE) PRIMARY KEY,
    MEMBERNAME VARCHAR2(40 BYTE) NOT NULL,
    MEMBERSHIPTYPE VARCHAR2(20 BYTE) NOT NULL,
    JOINING_DATE DATE NOT NULL,
    DURATION_MONTHS NUMBER,
    FEES NUMBER,
    REMARKS VARCHAR2(100 BYTE)
);

CREATE SEQUENCE GYM_SEQ
START WITH 10
INCREMENT BY 1
NOCYCLE;
